body .contentManage {
    height: auto;
    border: 1px solid #c5c5c5;
    overflow: hidden;
    padding: 5px; }
body .contentManage .t-title {
    width: 100%;
    text-align: center;
    font-size: 23px;
    font-weight: bold;
    margin-top: 10px; }
body .contentManage .table-head {
    width: 100%;
    height: 70px;
    border-bottom: 2px #434a50 solid; }
body .contentManage .table-head .operate {
    margin-top: 2%; }
body .contentManage .table-head .operate img {
    position: relative;
    left: 30px;
    top: 5px; }
body .contentManage .table-head .operate .add {
    background-color: #67c23a;
    margin-right:10%;
    float: right;
    color: white;
    font-size: 18px;
    width: 10%;
    height: 40px;
    border-radius: 4px;
    border: 1px #67c23a solid; }
body .contentManage .table-head .operate .add:hover {
    border: 1px #85ce61;
    background-color: #85ce61; }
body .contentManage .table-content {
    width: 100%;
    min-height: 800px;
    margin-top: 2%; }
body .contentManage .table-content table {
    margin: 0 auto;
    text-align: center;
    width: 80%;
    border: none;
    border-radius: 4px; }
body .contentManage .table-content table tr:hover {
    background-color: #E4E7ED; }
body .contentManage .table-content table th {
    padding: 5px; }
body .contentManage .table-content table td {
    width: auto;
    padding: 5px; }
body .contentManage .table-content table td .edit,
body .contentManage .table-content table td .delete,
body .contentManage .table-content table td .freeze,
body .contentManage .table-content table td .unfreeze,
body .contentManage .table-content table td .submit {
    outline: 0;
    font-size: 14px;
    border-radius: 3px;
    min-width: 80px;
    height: 30px; }
body .contentManage .table-content table td .edit {
    background-color: #fff;
    border-color: #dcdfe6; }
body .contentManage .table-content table td .edit:hover {
    background-color: #ecf5ff;
    border-color: #c6e2ff;
    color: #409EFF;
    transition-duration: 0.2s; }
body .contentManage .table-content table td .delete {
    background-color: #f56c6c;
    color: #fff;
    border: none; }
body .contentManage .table-content table td .delete:hover {
    background-color: #f78989;
    border-color: #f78989;
    transition-duration: 0.2s; }
body .contentManage .table-content table td .freeze {
    background-color: #409EFF;
    color: white;
    border: 1px #409EFF solid; }
body .contentManage .table-content table td .freeze:hover {
    border: 1px #66b1FF;
    background-color: #66b1ff; }
body .contentManage .table-content table td .unfreeze {
    background-color: #409EFF;
    color: white;
    border: 1px #409EFF solid; }
body .contentManage .table-content table td .unfreeze:hover {
    border: 1px #66b1FF;
    background-color: #66b1ff; }
body .contentManage .table-content table td .submit {
    background-color: #00BC9B;
    color: white;
    border: none; }
body .contentManage .table-content table td .submit:hover {
    outline: 0;
    background-color: #00B091; }
body .t-head{
    background: #e9e9e9;
}
body #foodTable{
    margin-top: 50px;
    border: 3px;             
}
.dataTables_filter{
    margin-right: 10%;
    width: 400px;
    margin-bottom: 2%;
}
.dataTables_filter label input{
    width: 70%;
    border-radius:4px;
    height: 30px;
    outline: 0;
    border:1px solid #CCCCCC;
}
.dataTables_filter label input:focus{
    outline: 0;
    border: 1px solid #ffa200;
    box-shadow: 0 0 5px #ffa200;
    transition-duration: 0.5s;
}
.dataTables_length{
    margin-left: 10%;
}

