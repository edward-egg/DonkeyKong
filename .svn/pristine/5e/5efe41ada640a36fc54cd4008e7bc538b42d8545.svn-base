body{
    .header{
        background-color: #fff;
        width: 100%;
        height: 50px;
        padding:0;
        height:80px;
        .title{
            display: block;
            float: left;
            font-size: 20px;
            font-weight: bolder;
            letter-spacing: 2px;
            margin-left:6%;
            span{
                position: relative;
                bottom: 25px;
            }
        }
        .nav{
            padding-top: 1%;
            display: block;
            margin-left: 20%;
            width: auto;
            height: inherit;
            ul{
                list-style-type: none;
                li{
                    float: left;
                    a{
                        color: black;
                        width: 80px;
                        height: 30px;
                        font-size: 18px;
                        line-height: 30px;
                        display: block;
                        text-align: center;
                        text-decoration: none;
                        margin-left: 8px;
                        border-bottom: 2px #EEB422 solid;
                        &:link{
                            color: black;
                        }
                        &:hover{
                            background-color: #EEB422;
                            transition-duration: 0.3s;
                            color: white;
                            font-weight: bold;
                            border-radius: 4px;
                        }
                        
                    }
                    
                }
            }
            .to-login{
                width: 200px;
                float: left;
                height: 50px;
                margin-left: 10%;
                .btn{ 
                    border: none;        
                    width: 100px;
                    background-color: #66CD00;
                    color: whitesmoke;
                    font-size: 18px;
                    border-radius: 5px;
                    height: 30px;
                    &:hover{
                        background-color:#90EE90; 
                    }
                }
                .register{
                    position: relative;
                    bottom: 30px;
                    left: 130px;
                }
            }
        }
        
    }
}