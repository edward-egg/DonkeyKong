body {
    .contentManage {
        height: auto;
        border: 1px solid #c5c5c5;
        overflow: hidden;
        padding: 5px;
        .t-title{
            width: 100%;
            text-align: center;
            font-size: 23px;
            font-weight: bold;
            margin-top:10px;
        }
        .table-head {
            width: 100%;
            height: 70px;
            border-bottom: 2px rgb(67, 74, 80) solid;
            .operate {
                margin-top: 2%;
                img {
                    position: relative;
                    left: 30px;
                    top: 5px;
                }
                .search {
                    width: 60%;
                    height: 40px;
                    border: 1px solid #dcdfe6;
                    border-radius: 4px;
                    padding-left: 25px;
                }
                .find {
                    background-color: #409EFF;
                    color: white;
                    font-size: 18px;
                    width: 10%;
                    height: 40px;
                    border-radius: 4px;
                    border: 1px #409EFF solid;
                    &:hover {
                        border: 1px #66b1FF;
                        background-color: #66b1ff;
                    }
                }
                .add {
                    background-color: #67c23a;
                    color: white;
                    font-size: 18px;
                    width: 10%;
                    height: 40px;
                    border-radius: 4px;
                    border: 1px #67c23a solid;
                    &:hover {
                        border: 1px #85ce61;
                        background-color: #85ce61;
                    }
                }
            }
        }
        .table-content {
            width: 100%;
            min-height: 800px;
            margin-top: 2%;
            table {
                margin:0 auto;
                text-align: center;
                width: 80%;
                border: none;
                border-radius: 4px;
                tr:hover{
                    background-color: #E4E7ED;
                }
                th{
                    padding: 5px;
                }
                td {
                    width:auto;
                    padding: 5px;
                    .edit,
                    .delete,
                    .freeze ,
                    .submit{
                        outline: 0;
                        font-size: 14px;
                        border-radius: 3px;
                        min-width:80px;
                        height: 30px;
                    }
                    .edit {
                        background-color: #fff;
                        border-color: #dcdfe6;
                        &:hover {
                            background-color: #ecf5ff;
                            border-color: #c6e2ff;
                            color: #409EFF;
                            transition-duration: 0.2s;
                        }
                    }
                    .delete {
                        background-color: #f56c6c;
                        color: #fff;
                        border: none;
                        &:hover {
                            background-color: #f78989;
                            border-color: #f78989;
                            transition-duration: 0.2s;
                        }
                    }
                    .freeze{
                        background-color: #409EFF;
                        color: white;                       
                        border: 1px #409EFF solid;
                        &:hover {
                            border: 1px #66b1FF;
                            background-color: #66b1ff;
                        }
                    }
                    .submit{
                        background-color: #00BC9B;
                        color: white;                       
                        border: none;
                         
                        &:hover {
                            outline: 0;
                            background-color: #00B091;
                        }
                    }
                }
            }
        }
    }
}