DROP TRIGGER IF EXISTS `area_AFTER_DELETE`;
DELIMITER ;;
CREATE TRIGGER `area_AFTER_DELETE` AFTER DELETE ON `areas` FOR EACH ROW BEGIN
delete from food_store where area_id = old.area_id;
delete from hotel where area_id = old.area_id;
delete from scenic where area_id = old.area_id;
END
;;
DELIMITER ;
DROP TRIGGER IF EXISTS `food_store_AFTER_DELETE`;
DELIMITER ;;
CREATE TRIGGER `food_store_AFTER_DELETE` AFTER DELETE ON `food_store` FOR EACH ROW BEGIN
delete from food_store_comment where food_store_id = old.food_store_id;
END
;;
DELIMITER ;
DROP TRIGGER IF EXISTS `hotel_AFTER_DELETE`;
DELIMITER ;;
CREATE TRIGGER `hotel_AFTER_DELETE` AFTER DELETE ON `hotel` FOR EACH ROW BEGIN
delete from hotel_book where hotel_id = old.hotel.id;
delete from hotel_room where hotel_id = old.hotel.id;
END
;;
DELIMITER ;
DROP TRIGGER IF EXISTS `hotel_room_AFTER_DELETE`;
DELIMITER ;;
CREATE TRIGGER `hotel_room_AFTER_DELETE` AFTER DELETE ON `hotel_room` FOR EACH ROW BEGIN
delete from hotel_book where room_id = old.room_id;
END
;;
DELIMITER ;
DROP TRIGGER IF EXISTS `note_AFTER_DELETE`;
DELIMITER ;;
CREATE TRIGGER `note_AFTER_DELETE` AFTER DELETE ON `note` FOR EACH ROW BEGIN
	delete from note_comment where note_id = old.note_id;
END
;;
DELIMITER ;
DROP TRIGGER IF EXISTS `tour_AFTER_DELETE`;
DELIMITER ;;
CREATE TRIGGER `tour_AFTER_DELETE` AFTER DELETE ON `tour` FOR EACH ROW BEGIN
delete from tour_apply where tour_id = old.tour_id;
delete from tour_comment where tour_id = old.tour_id;
END
;;
DELIMITER ;
DROP TRIGGER IF EXISTS `users_AFTER_DELETE`;
DELIMITER ;;
CREATE TRIGGER `users_AFTER_DELETE` AFTER DELETE ON `users` FOR EACH ROW BEGIN
delete from food_store_comment where user_id = old.user_id;
delete from hotel_book where user_id = old.user_id;
delete from note where user_id = old.user_id;
delete from notification where user_id = old.user_id;
delete from tour_apply where user_id = old.user_id;
delete from tour_comment where user_id = old.user_id;
END
;;
DELIMITER ;
