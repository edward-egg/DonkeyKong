    color: whitesmoke;
    font-size: 18px;
    border-radius: 5px;
    height: 30px; }
body .header .nav .to-login .btn:hover {
    background-color: #90EE90; }
body .header .nav .to-login .register {
    position: relative;
    bottom: 30px;
    left: 130px; }
body .food-detail {
