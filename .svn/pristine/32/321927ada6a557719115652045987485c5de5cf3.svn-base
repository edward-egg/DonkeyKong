body .header {
    background-color: #fff;
    width: 100%;
    height: 50px;
    padding: 0;
    height: 80px; }
body .header .title {
    display: block;
    float: left;
    font-size: 20px;
    font-weight: bolder;
    letter-spacing: 2px;
    margin-left: 6%; }
body .header .title span {
    position: relative;
    bottom: 25px; }
body .header .nav {
    padding-top: 1%;
    display: block;
    margin-left: 20%;
    width: auto;
    height: inherit; }
body .header .nav ul {
    list-style-type: none; }
body .header .nav ul li {
    float: left; }
body .header .nav ul li a {
    color: black;
    width: 80px;
    height: 30px;
    font-size: 18px;
    line-height: 30px;
    display: block;
