body {
    width: 100%;
    height: 100%;
    margin: 0;
    padding: 0;
    .header {
        width: 100%;
        height: 150px;
        background-color: rgb(84, 92, 100);
        .title {
            position: absolute;
            top: 4%;
            left: 35%;
            color: white;
            font-size: 50px;
            font-weight: bold;
            span{
                position: relative;
                bottom: 30px;
            }
        }
        .admin {
            float: left;
            color: white;
            font-size: 25px;
            font-weight: bold;
            margin: 2% 0 0 2%;
        }
        .logout {
            float: right;
            width: 8%;
            height: 40px;
            position: relative;
            top: 20%;
            right: 5%;
            border-radius: 5px;
            background-color: #909399;
            color: #fff;
            font-size: 18px;
            border: none;
            margin-left: 10px;
            &:hover {
                background-color: #a6a9ad;
                transition-duration: 0.2s;
            }
        }
        .to-main {
            margin-left: 5px;
            outline: 0;
            float: right;
            width: 8%;
            height: 40px;
            position: relative;
            top: 20%;
            right: 5%;
            border-radius: 5px;
            background-color: #00BC9B;
            color: #fff;
            font-size: 18px;
            border: none;
            &:hover {
                outline: 0;
                background-color: #00B091;
                transition-duration: 0.2s;
            }
        }
    }
    .banner {
        background-color: rgb(84, 92, 100);
        float: left;
        width: 10%;
        height: 100%;
        ul {
            // margin-top: 20%;
            position: relative;
            right: 40px;
            list-style-type: none;
            width: 100%;
            li {
                text-align: center;
                margin-top: 10%;
                width: 100%;
                height: 50px; // border-bottom: 1px wheat solid;
                &:hover {
                    background-color: rgb(67, 74, 80);
                }
                .icon-menu {
                    width: 20px;
                    height: 20px;
                }
                a {
                    display: block;
                    padding-top: 8%; // margin-top: 30%;
                    font-size: 1em;
                    text-decoration: none;
                    color: white;
                    &:hover {
                        font-weight: bold;
                        color: rgb(255, 208, 75);
                    }
                }
            }
        }
    }
    .body {
        width: 80%;
        min-height: 800px;
        height: auto;
        float: left;
    }
}