body {
    overflow-x: hidden; }
body .header {
    background-color: #fff;
    width: 100%;
    height: 50px;
    padding: 0;
    height: 80px; }
body .header .title {
    display: block;
    float: left;
    font-size: 20px;
    font-weight: bolder;
    letter-spacing: 2px;
    margin-left: 6%; }
body .header .title span {
    position: relative;
    bottom: 25px; }
body .header .nav {
    padding-top: 1%;
    display: block;
    margin-left: 20%;
    width: auto;
    height: inherit; }
body .header .nav ul {
    list-style-type: none; }
body .header .nav ul li {
    float: left; }
body .header .nav ul li a {
    color: black;
    width: 80px;
    height: 30px;
    font-size: 18px;
    line-height: 30px;
    display: block;
    text-align: center;
    text-decoration: none;
    margin-left: 8px;
    border-bottom: 2px #EEB422 solid; }
body .header .nav ul li a:link {
    color: black; }
body .header .nav ul li a:hover {
    background-color: #EEB422;
    transition-duration: 0.3s;
    color: white;
    font-weight: bold;
    border-radius: 4px; }
body .header .nav .to-login {
    width: 200px;
    float: left;
    height: 50px;
    margin-left: 10%; }
body .header .nav .to-login .btn {
    border: none;
    width: 100px;
    background-color: #66CD00;
    color: whitesmoke;
    font-size: 18px;
    border-radius: 5px;
    height: 30px; }
body .header .nav .to-login .btn:hover {
    background-color: #90EE90; }
body .header .nav .to-login .register {
    position: relative;
    bottom: 30px;
    left: 130px; }

body .travel-list {
    width: 60%;
    margin: 2% auto;
    margin-top: 70px;
    height: auto;
    border: 1px solid #E4E7ED;
    padding: 5px; }
body .travel-list .toolbar {
    width: 100%;
    height: 60px;
    border-radius: 5px;
    background-color: #a6a9ad; }
body .travel-list .toolbar .travel-sort {
    float: left;
    width: 40%;
    margin-top: 1%;
    margin-left: 20px; }
body .travel-list .toolbar .travel-sort span {
    font-size: 20px;
    font-weight: bold;
    color: whitesmoke; }
body .travel-list .toolbar .travel-sort .sort-btn {
    background-color: #409EFF;
    color: white;
    font-size: 14px;
    font-weight: bold;
    width: 25%;
    height: 30px;
    border-radius: 4px;
    border: 1px #409EFF solid; }
body .travel-list .toolbar .travel-sort .sort-btn:hover {
    cursor: pointer;
    transition-duration: 0.4s;
    border: 1px #66b1FF;
    background-color: #66b1ff; }
body .travel-list .travel-note {
    display:flex;
    justify-content: center;
    flex-wrap: wrap;
    margin-top: 8px;
    width: 100%;
    padding: 5px; }
body .travel-list .travel-note .travel-item {
    border: 1px solid #f1f1f1;
    margin: 15px 10px;
    text-align: center;
    width:25%; }
body .travel-list .travel-note .travel-item:hover {
    cursor: pointer;
    transition-duration: 0.3s;
    background-color: #F5F5F5; }
body .travel-list .travel-note .travel-item .t-head{
    width: 100%;
    height: 100px;
    text-align: center;
    border: 1px solid #cccccc;
}
body .travel-list .travel-note .travel-item .t-head #t-name{
    display: block;
    margin-top:10%;
    font-size: 20px;
    font-weight: bolder;
    letter-spacing: 5px;
}
body .travel-list .travel-note .travel-item .t-body{
    padding: 5px;
}
body .travel-list .travel-note .travel-item .t-body .icon{
    width: 20px;
    height: 20px;
}
body .travel-list .travel-note .travel-item .t-body .cash{
    width: 35px;
    height: 35px;
}
body .travel-list .travel-note .travel-item .money{
    color: #f4ea2a;
    font-size: 25px;
    font-weight: bold;
}
/*# sourceMappingURL=travelPage.css.map */
