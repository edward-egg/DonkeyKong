package util;

import java.sql.Timestamp;
import java.util.Date;

/**
 *
 * @author Jason
 */
public class DateUtils {
    //生成当前时间的时间戳
    public static Timestamp getTimestamp() {
        return new Timestamp(new Date().getTime());
    }
}
